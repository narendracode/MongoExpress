<!doctype html>
<html>
<head>
	 <% include ./partials/head %>
	 <style>
        body{
            padding-top: 120px;
        }
    </style>
</head>
<body>

<% include ./partials/navbar %>

<div class="container">
<div class="row">
<% include ./partials/navbar-left-fixed %>
    <div class="col-sm-9">
        <div class="row">
		<!-- LOCAL INFORMATION -->
		<div class="col-sm-6">
			<div class="well">
				<h3><span class="fa fa-user"></span> Local</h3>
					<p>
						<strong>id</strong>: <%= user._id %><br>
						<strong>email</strong>: <%= user.local.email %><br>
						<strong>password</strong>: <%= user.local.password %>
					</p>

			</div>
		</div>
        </div>
         <div class="row">
		<!-- FACEBOOK INFORMATION -->
		<div class="col-sm-6">
			<div class="well">
				<h3 class="text-primary"><span class="fa fa-facebook"></span> Facebook</h3>
				<p>
					<strong>id</strong>: <%= user.facebook.id %><br>
					<strong>token</strong>: <%= user.facebook.token %><br>
					<strong>email</strong>: <%= user.facebook.email %><br>
					<strong>name</strong>: <%= user.facebook.name %>
				</p>
			</div>
		</div>
		</div>
		<div class="row">
		<div class="col-sm-6">
			<div class="well">
				<h3 class="text-primary"><span class="fa fa-twitter"></span> Twitter</h3>
				<p>
					<strong>id</strong>: <%= user.twitter.id %><br>
					<strong>token</strong>: <%= user.twitter.token %><br>
					<strong>username</strong>: <%= user.twitter.username %><br>
					<strong>displayName</strong>: <%= user.twitter.displayName %>
				</p>
			</div>
		</div>
	</div>
    </div>   
</div>
	
</div><!-- container-->
 <!-- Footer -->
   <% include ./partials/footer %>
   <% include ./partials/load-js-afterbody %>
</body>
</html>
